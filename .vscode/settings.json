{
    "vs-code-prettier-eslint.prettierLast": true,
    "eslint.validate": [
        "javascript",
        "typescript",
        "jsx",
        "tsx"
    ],
    "eslint.workingDirectories": [
        ".eslintrc.json"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.formatOnSave": true, 
    "eslint.alwaysShowStatus": true, 
    "files.autoSave": "onFocusChange",
    "cSpell.words": [
        "Swiper"
    ]
}